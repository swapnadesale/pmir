<script language="javascript">
	function checkEnter()
	{
		if(event.keyCode==13)
			checkempty();
	}

	function checkempty()
	{
		var input = document.form1.topic.value;
		if (input=="")
		{
			alert("请输入查询！");
			return false;
		}
		document.form1.query.value = input;
		return true;
	}
